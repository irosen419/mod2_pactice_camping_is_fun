<h1> New camper </h1>
<% if flash[:errors]%>
    <% flash[:errors].each do |error|%>
        <%= error %>
    <% end %>
<% end %>
    
<%= form_for @camper do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>

    <%= f.label :age %><br>
    <%= f.number_field :age %><br>

    <%= f.submit %>
<% end %>
